<owl:Class rdf:ID="TIME-INTERVAL">
  <rdfs:subClassOf rdf:resource="#TIME-SPECIFICATION" />

  <rdfs:subClassOf>
   <owl:Restriction>
     <owl:onProperty rdf:resource="#HAS-UNIT-OF-MEASURE"/>
