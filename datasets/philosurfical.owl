       <owl:Class rdf:ID="MANIFESTATION"/>
     </owl:allValuesFrom>
   </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:comment>FRBR : the information carrier that exemplifies a manifestation, i.e. indirectly a production plan
  </rdfs:comment>
</owl:Class>
<owl:ObjectProperty rdf:ID="EXEMPLIFIES">
  <rdfs:domain rdf:resource="#ITEM" />
</owl:ObjectProperty>

